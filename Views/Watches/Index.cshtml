@model IEnumerable<ADKT_WebProject.Models.Identities.Watch>

@{
    ViewBag.Title = "ADKT.com";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Danh Sách Đồng Hồ</h2>

<p>
    
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Brand.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.glass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.waterproof)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.strap)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Brand.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.gender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.glass)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.waterproof)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.strap)
        </td>
        <td>
            
            @Html.ActionLink("Details", "Details", new { id=item.Id }) 
           
        </td>
    </tr>
}

</table>
